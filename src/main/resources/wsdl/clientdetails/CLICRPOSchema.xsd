<xsd:schema targetNamespace="http://www.csc.smart/bo/schemas/CLICRPO" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:boerr="http://www.csc.smart/bo/schemas/Error">
   <xsd:import namespace="http://www.csc.smart/bo/schemas/Error" schemaLocation="BOError.xsd"/>
   <xsd:element name="CLICRPO_REC">
